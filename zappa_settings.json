{
    "dev": {
        "app_function": "web.__init__.APP",
        "aws_region": "ap-southeast-2",
        "profile_name": null,
        "project_name": "irs",
        "runtime": "python3.6",
        "s3_bucket": "irs-zappa",
        "environment_variables": {"route_prefix": "dev"},
        "events": [{
            "function": "events.image_handler.process_image",
            "event_source": {
                "arn": "arn:aws:s3:::irs-images",
                "events": [
                    "s3:ObjectCreated:*"
                ]
            }
        }]
    }
}